{
  "name": "keeperapi",
  "version": "0.2.0",
  "browser": "dist/index.es.js",
  "main": "dist/index.cjs.js",
  "browsertest": "dist/test.cjs.js",
  "types": "dist/node/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "api-build": "rollup -c",
    "api-watch": "rollup -cw",
    "update-proto-es6": "pbjs -t static-module -w es6 -o src/proto.js ../../keeperapp-protobuf/*.proto && pbts -o src/proto.d.ts src/proto.js",
    "update-proto-cjs": "pbjs -t json-module -w commonjs -o src/proto.js ../../keeperapp-protobuf/*.proto && pbjs -t static-module -w commonjs ../../keeperapp-protobuf/*.proto | pbts -o src/proto.d.ts -",
    "link-local": "yarn link",
    "test": "ts-node test/test.ts",
    "test-new-login": "ts-node test/new_login.ts",
    "jsttst": "jest"
  },
  "license": "ISC",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "form-data": "^3.0.0",
    "protobufjs": "^6.8.8"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.12.31",
    "jest": "^24.8.0",
    "rollup": "^1.16.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.5.2"
  }
}
