{
  "name": "keeperapi",
  "version": "0.3.0-beta.19",
  "browser": "dist/index.es.js",
  "main": "dist/index.cjs.js",
  "browsertest": "dist/test.cjs.js",
  "types": "dist/node/index.d.ts",
  "files": [
    "dist"
  ],
  "protobufFiles": "../../keeperapp-protobuf/APIRequest.proto ../../keeperapp-protobuf/AccountSummary.proto ../../keeperapp-protobuf/BIApiRequest.proto ../../keeperapp-protobuf/breachwatch.proto ../../keeperapp-protobuf/client.proto ../../keeperapp-protobuf/externalservice.proto ../../keeperapp-protobuf/folder.proto ../../keeperapp-protobuf/push.proto ../../keeperapp-protobuf/record.proto ../../keeperapp-protobuf/servicelogger.proto ../../keeperapp-protobuf/ssocloud.proto ../../keeperapp-protobuf/token.proto ../../keeperapp-protobuf/upsell.proto",
  "scripts": {
    "start": "rollup -cw",
    "build": "rollup -c && cp src/proto.d.ts src/browser/ecies.browserified.js dist",
    "update-proto:es6": "pbjs -t static-module -w es6 -o src/proto.js $npm_package_protobufFiles && pbts -o src/proto.d.ts src/proto.js",
    "update-proto:cjs": "pbjs -t json-module -w commonjs -o src/proto.js $npm_package_protobufFiles && pbjs -t static-module -w commonjs $npm_package_protobufFiles | pbts -o src/proto.d.ts -",
    "link-local": "yarn link",
    "publish:beta": "node ./scripts/verifyBetaVersion.js && npm publish --tag beta",
    "test": "ts-node test/test.ts",
    "test-new-login": "ts-node test/new_login.ts",
    "test-ws_push": "ts-node test/ws_push.ts",
    "test-sso": "ts-node test/test-sso-and-logger.ts",
    "push-agent": "ts-node test/keeperPush.ts",
    "test:2": "jest",
    "types": "tsc --watch",
    "types:ci": "tsc"
  },
  "license": "ISC",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "faye-websocket": "^0.11.3",
    "form-data": "^3.0.0",
    "node-rsa": "^1.0.8",
    "open": "^7.0.3",
    "protobufjs": "^6.8.8"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.12.31",
    "jest": "^24.8.0",
    "rollup": "^1.16.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
